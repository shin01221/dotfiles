# Autosuggestions (grey-ish, subtle)
set fish_color_autosuggestion 52443c

# Ctrl+C feedback
set fish_color_cancel ffb4ab

# Commands (main accent)
set fish_color_command ffb689 --bold

# Comments
set fish_color_comment 9f8d83

# Current working directory
set fish_color_cwd e5bfa9
set fish_color_cwd_root ffb4ab

# Command separator like `;` or `&&`
set fish_color_end cbc992

# Errors
set fish_color_error ffb4ab

# Special characters like escapes
set fish_color_escape 5c4130

# Selected command entry in history search
set fish_color_history_current --bold

# Host appearance
set fish_color_host d7c2b8
set fish_color_host_remote ffb4ab

# Keywords like `if`, `for`
set fish_color_keyword ffb689

# Highlight results
set fish_color_match cbc992

# Default text
set fish_color_normal f0dfd7

# Operators like = + =>
set fish_color_operator e5bfa9

# Options `--flag`
set fish_color_option e5bfa9

# Params/arguments
set fish_color_param cbc992

# Strings
set fish_color_quote e5bfa9

# Redirect symbols `>`, `<`, `|`
set fish_color_redirection 6f3810

# Search highlight colors
set fish_color_search_match f0dfd7 --background=312823

# Selection in command line
set fish_color_selection f0dfd7 --background=312823

set fish_color_status ffb4ab
set fish_color_user ffb689
set fish_color_valid_path e5bfa9 --underline

#######################
# Pager (tab completion menu)
#######################
set fish_pager_color_background 1a120d
set fish_pager_color_completion d7c2b8
set fish_pager_color_description 9f8d83
set fish_pager_color_prefix ffb689 --bold --underline
set fish_pager_color_progress e5bfa9
set fish_pager_color_secondary_background 221a15
set fish_pager_color_secondary_completion f0dfd7
set fish_pager_color_secondary_description 52443c
set fish_pager_color_secondary_prefix e5bfa9
set fish_pager_color_selected_background --background=312823
set fish_pager_color_selected_completion ffb689
set fish_pager_color_selected_description f0dfd7
set fish_pager_color_selected_prefix ffb689
