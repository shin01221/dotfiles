# Autosuggestions (grey-ish, subtle)
set fish_color_autosuggestion 50453a

# Ctrl+C feedback
set fish_color_cancel ffb4ab

# Commands (main accent)
set fish_color_command fbb974 --bold

# Comments
set fish_color_comment 9d8e81

# Current working directory
set fish_color_cwd e1c1a3
set fish_color_cwd_root ffb4ab

# Command separator like `;` or `&&`
set fish_color_end becc9b

# Errors
set fish_color_error ffb4ab

# Special characters like escapes
set fish_color_escape 58432c

# Selected command entry in history search
set fish_color_history_current --bold

# Host appearance
set fish_color_host d5c3b5
set fish_color_host_remote ffb4ab

# Keywords like `if`, `for`
set fish_color_keyword fbb974

# Highlight results
set fish_color_match becc9b

# Default text
set fish_color_normal eee0d5

# Operators like = + =>
set fish_color_operator e1c1a3

# Options `--flag`
set fish_color_option e1c1a3

# Params/arguments
set fish_color_param becc9b

# Strings
set fish_color_quote e1c1a3

# Redirect symbols `>`, `<`, `|`
set fish_color_redirection 683d00

# Search highlight colors
set fish_color_search_match eee0d5 --background=302921

# Selection in command line
set fish_color_selection eee0d5 --background=302921

set fish_color_status ffb4ab
set fish_color_user fbb974
set fish_color_valid_path e1c1a3 --underline

#######################
# Pager (tab completion menu)
#######################
set fish_pager_color_background 19120c
set fish_pager_color_completion d5c3b5
set fish_pager_color_description 9d8e81
set fish_pager_color_prefix fbb974 --bold --underline
set fish_pager_color_progress e1c1a3
set fish_pager_color_secondary_background 211a14
set fish_pager_color_secondary_completion eee0d5
set fish_pager_color_secondary_description 50453a
set fish_pager_color_secondary_prefix e1c1a3
set fish_pager_color_selected_background --background=302921
set fish_pager_color_selected_completion fbb974
set fish_pager_color_selected_description eee0d5
set fish_pager_color_selected_prefix fbb974
