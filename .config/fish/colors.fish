# Autosuggestions (grey-ish, subtle)
set fish_color_autosuggestion 53433e

# Ctrl+C feedback
set fish_color_cancel ffb4ab

# Commands (main accent)
set fish_color_command ffb59c --bold

# Comments
set fish_color_comment a08d86

# Current working directory
set fish_color_cwd e7bdb0
set fish_color_cwd_root ffb4ab

# Command separator like `;` or `&&`
set fish_color_end d6c68e

# Errors
set fish_color_error ffb4ab

# Special characters like escapes
set fish_color_escape 5d4036

# Selected command entry in history search
set fish_color_history_current --bold

# Host appearance
set fish_color_host d8c2bb
set fish_color_host_remote ffb4ab

# Keywords like `if`, `for`
set fish_color_keyword ffb59c

# Highlight results
set fish_color_match d6c68e

# Default text
set fish_color_normal f1dfd9

# Operators like = + =>
set fish_color_operator e7bdb0

# Options `--flag`
set fish_color_option e7bdb0

# Params/arguments
set fish_color_param d6c68e

# Strings
set fish_color_quote e7bdb0

# Redirect symbols `>`, `<`, `|`
set fish_color_redirection 72351f

# Search highlight colors
set fish_color_search_match f1dfd9 --background=322824

# Selection in command line
set fish_color_selection f1dfd9 --background=322824

set fish_color_status ffb4ab
set fish_color_user ffb59c
set fish_color_valid_path e7bdb0 --underline

#######################
# Pager (tab completion menu)
#######################
set fish_pager_color_background 1a110f
set fish_pager_color_completion d8c2bb
set fish_pager_color_description a08d86
set fish_pager_color_prefix ffb59c --bold --underline
set fish_pager_color_progress e7bdb0
set fish_pager_color_secondary_background 231a16
set fish_pager_color_secondary_completion f1dfd9
set fish_pager_color_secondary_description 53433e
set fish_pager_color_secondary_prefix e7bdb0
set fish_pager_color_selected_background --background=322824
set fish_pager_color_selected_completion ffb59c
set fish_pager_color_selected_description f1dfd9
set fish_pager_color_selected_prefix ffb59c
