# Autosuggestions (grey-ish, subtle)
set fish_color_autosuggestion 524343

# Ctrl+C feedback
set fish_color_cancel ffb4ab

# Commands (main accent)
set fish_color_command ffb2b6 --bold

# Comments
set fish_color_comment 9f8c8c

# Current working directory
set fish_color_cwd e6bdbe
set fish_color_cwd_root ffb4ab

# Command separator like `;` or `&&`
set fish_color_end e7c08e

# Errors
set fish_color_error ffb4ab

# Special characters like escapes
set fish_color_escape 5c3f41

# Selected command entry in history search
set fish_color_history_current --bold

# Host appearance
set fish_color_host d7c1c2
set fish_color_host_remote ffb4ab

# Keywords like `if`, `for`
set fish_color_keyword ffb2b6

# Highlight results
set fish_color_match e7c08e

# Default text
set fish_color_normal f0dede

# Operators like = + =>
set fish_color_operator e6bdbe

# Options `--flag`
set fish_color_option 723339

# Params/arguments
set fish_color_param e7c08e

# Strings
set fish_color_quote 5c421a

# Redirect symbols `>`, `<`, `|`
set fish_color_redirection 723339

# Search highlight colors
set fish_color_search_match f0dede --background=322828

# Selection in command line
set fish_color_selection f0dede --background=322828

set fish_color_status ffb4ab
set fish_color_user ffb2b6
set fish_color_valid_path e6bdbe --underline

#######################
# Pager (tab completion menu)
#######################
set fish_pager_color_background 1a1112
set fish_pager_color_completion d7c1c2
set fish_pager_color_description 9f8c8c
set fish_pager_color_prefix ffb2b6 --bold --underline
set fish_pager_color_progress e6bdbe
set fish_pager_color_secondary_background 22191a
set fish_pager_color_secondary_completion f0dede
set fish_pager_color_secondary_description 524343
set fish_pager_color_secondary_prefix e6bdbe
set fish_pager_color_selected_background --background=322828
set fish_pager_color_selected_completion ffb2b6
set fish_pager_color_selected_description f0dede
set fish_pager_color_selected_prefix ffb2b6
