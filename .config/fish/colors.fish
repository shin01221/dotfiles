# Autosuggestions (grey-ish, subtle)
set fish_color_autosuggestion 514346

# Ctrl+C feedback
set fish_color_cancel ffb4ab

# Commands (main accent)
set fish_color_command ffb1c5 --bold

# Comments
set fish_color_comment 9e8c8f

# Current working directory
set fish_color_cwd e3bdc5
set fish_color_cwd_root ffb4ab

# Command separator like `;` or `&&`
set fish_color_end eebd92

# Errors
set fish_color_error ffb4ab

# Special characters like escapes
set fish_color_escape 5b3f46

# Selected command entry in history search
set fish_color_history_current --bold

# Host appearance
set fish_color_host d6c2c5
set fish_color_host_remote ffb4ab

# Keywords like `if`, `for`
set fish_color_keyword ffb1c5

# Highlight results
set fish_color_match eebd92

# Default text
set fish_color_normal efdfe1

# Operators like = + =>
set fish_color_operator e3bdc5

# Options `--flag`
set fish_color_option e3bdc5

# Params/arguments
set fish_color_param eebd92

# Strings
set fish_color_quote e3bdc5

# Redirect symbols `>`, `<`, `|`
set fish_color_redirection 703346

# Search highlight colors
set fish_color_search_match efdfe1 --background=312829

# Selection in command line
set fish_color_selection efdfe1 --background=312829

set fish_color_status ffb4ab
set fish_color_user ffb1c5
set fish_color_valid_path e3bdc5 --underline

#######################
# Pager (tab completion menu)
#######################
set fish_pager_color_background 191113
set fish_pager_color_completion d6c2c5
set fish_pager_color_description 9e8c8f
set fish_pager_color_prefix ffb1c5 --bold --underline
set fish_pager_color_progress e3bdc5
set fish_pager_color_secondary_background 22191b
set fish_pager_color_secondary_completion efdfe1
set fish_pager_color_secondary_description 514346
set fish_pager_color_secondary_prefix e3bdc5
set fish_pager_color_selected_background --background=312829
set fish_pager_color_selected_completion ffb1c5
set fish_pager_color_selected_description efdfe1
set fish_pager_color_selected_prefix ffb1c5
