# Autosuggestions (grey-ish, subtle)
set fish_color_autosuggestion 534341

# Ctrl+C feedback
set fish_color_cancel ffb4ab

# Commands (main accent)
set fish_color_command ffb4ab --bold

# Comments
set fish_color_comment a08c8a

# Current working directory
set fish_color_cwd e7bdb8
set fish_color_cwd_root ffb4ab

# Command separator like `;` or `&&`
set fish_color_end e0c38c

# Errors
set fish_color_error ffb4ab

# Special characters like escapes
set fish_color_escape 5d3f3c

# Selected command entry in history search
set fish_color_history_current --bold

# Host appearance
set fish_color_host d8c2bf
set fish_color_host_remote ffb4ab

# Keywords like `if`, `for`
set fish_color_keyword ffb4ab

# Highlight results
set fish_color_match e0c38c

# Default text
set fish_color_normal f1dedc

# Operators like = + =>
set fish_color_operator e7bdb8

# Options `--flag`
set fish_color_option e7bdb8

# Params/arguments
set fish_color_param e0c38c

# Strings
set fish_color_quote e7bdb8

# Redirect symbols `>`, `<`, `|`
set fish_color_redirection 73332d

# Search highlight colors
set fish_color_search_match f1dedc --background=322826

# Selection in command line
set fish_color_selection f1dedc --background=322826

set fish_color_status ffb4ab
set fish_color_user ffb4ab
set fish_color_valid_path e7bdb8 --underline

#######################
# Pager (tab completion menu)
#######################
set fish_pager_color_background 1a1110
set fish_pager_color_completion d8c2bf
set fish_pager_color_description a08c8a
set fish_pager_color_prefix ffb4ab --bold --underline
set fish_pager_color_progress e7bdb8
set fish_pager_color_secondary_background 231918
set fish_pager_color_secondary_completion f1dedc
set fish_pager_color_secondary_description 534341
set fish_pager_color_secondary_prefix e7bdb8
set fish_pager_color_selected_background --background=322826
set fish_pager_color_selected_completion ffb4ab
set fish_pager_color_selected_description f1dedc
set fish_pager_color_selected_prefix ffb4ab
